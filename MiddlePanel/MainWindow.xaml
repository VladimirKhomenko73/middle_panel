<Window x:Class="MiddlePanel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MiddlePanel"
        xmlns:viewModel="clr-namespace:MiddlePanel.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="860" Width="1240" WindowStyle="None">
    <Window.DataContext>
        <viewModel:MainWindowViewModel/>
    </Window.DataContext>
    <Window.CommandBindings>
        <CommandBinding Command="ApplicationCommands.Close"
                        Executed="CloseCommandHandler"/>
    </Window.CommandBindings>
    <Window.InputBindings>
        <KeyBinding Command="ApplicationCommands.Close" Key="Esc"/>
    </Window.InputBindings>
    <Grid Background="Black">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="1.25*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Grid x:Name="RPM1L" HorizontalAlignment="Left" Height="226" Margin="12,12,0,0" VerticalAlignment="Top" Width="225" Grid.Column="3" Grid.Row="0">
            <Grid x:Name="RPM1LBackgroundLayer" HorizontalAlignment="Left" Height="225" VerticalAlignment="Top" Width="225">
                <Image x:Name="RPM1LBackground" Source="Resourse/RPM1.png" HorizontalAlignment="Left" Height="225" VerticalAlignment="Top" Width="225"/>
            </Grid>
            <Grid x:Name="RPM1LArrowLayer" HorizontalAlignment="Left" Height="225" VerticalAlignment="Top" Width="225" RenderTransformOrigin="0.497,0.499">
                <Grid.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="{Binding ngLAngle}"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Grid.RenderTransform>
                <Image x:Name="RPM1LArrow" Source="Resourse/RPM Arrow.png" Margin="30,106,82,109" RenderTransformOrigin="0.76,0.481">
                    <Image.RenderTransform>
                        <TransformGroup>
                            <SkewTransform/>
                            <RotateTransform Angle="150.351"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Image.RenderTransform>
                </Image>
            </Grid>
        </Grid>

        <Grid x:Name="RPM1R" HorizontalAlignment="Left" Height="226" Margin="12,12,0,0" VerticalAlignment="Top" Width="225" Grid.Column="4" Grid.Row="0">
            <Grid x:Name="RPM1RBackgroundLayer" HorizontalAlignment="Left" Height="225" VerticalAlignment="Top" Width="225">
                <Image x:Name="RPM1RBackground" Source="Resourse/RPM1.png" HorizontalAlignment="Left" Height="225" VerticalAlignment="Top" Width="225"/>
            </Grid>
            <Grid x:Name="RPM1RArrowLayer" HorizontalAlignment="Left" Height="225" VerticalAlignment="Top" Width="225" RenderTransformOrigin="0.497,0.499">
                <Grid.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="{Binding ngLAngle}"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Grid.RenderTransform>
                <Image x:Name="RPM1RArrow" Source="Resourse/RPM Arrow.png" Margin="30,106,82,109" RenderTransformOrigin="0.76,0.481">
                    <Image.RenderTransform>
                        <TransformGroup>
                            <SkewTransform/>
                            <RotateTransform Angle="150.351"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Image.RenderTransform>
                </Image>
            </Grid>
        </Grid>


        <!--Image x:Name="RPM1RBackground" Source="Resourse/RPM1.png" HorizontalAlignment="Left" Height="226" Margin="12,12,0,0" VerticalAlignment="Top" Width="225" Grid.Column="1" Grid.Row="0"/>
        <Image x:Name="RPM1RArrow" Source="Resourse/RPM Arrow.png" RenderTransformOrigin="0.76,0.481" Margin="506.5,360,105,482" Grid.Column="1">
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="150.351"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image-->

        <Grid x:Name="RPM2L" HorizontalAlignment="Left" Height="226" Margin="12,12,0,0" VerticalAlignment="Top" Width="226" Grid.Column="3" Grid.Row="1">
